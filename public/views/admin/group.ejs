<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<title><%= title %></title>
	<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport" />
	<meta content="" name="description" />
	<meta content="" name="author" />
	<link rel="icon" href="../../assets-home/home/finfine.png" type="image/gif">
	<!-- ================== BEGIN BASE CSS STYLE ================== -->
	<link href="../../assets/css/transparent/app.min.css" rel="stylesheet" />
	<!-- ================== END BASE CSS STYLE ================== -->
	
	<!-- ================== BEGIN PAGE LEVEL STYLE ================== -->
	<link href="../../assets/plugins/smartwizard/dist/css/smart_wizard.css" rel="stylesheet" />
</head>
<body>
	<%- include("../../frame/loader") -%>

	<!-- begin #page-container -->
	<div id="page-container" class="fade page-sidebar-fixed page-header-fixed">
		<%- include("../../frame/header") -%>
		<%- include("../../frame/sidebar") -%>
		
        <!-- begin #content -->
		<div id="content" class="content">
			<!-- begin breadcrumb -->
			<ol class="breadcrumb float-xl-right">
				<li class="breadcrumb-item"><a href="javascript:;">Management</a></li>
				<li class="breadcrumb-item active">Group</li>
			</ol>
			<!-- end breadcrumb -->
			<!-- begin page-header -->
			<h1 class="page-header">Group</h1>
            <!-- end page-header -->
            <!-- begin panel -->
			<div class="panel panel-inverse">
				<h3 class="p-10">Group Information</h3>
				<!-- begin panel-body -->
				<div class="panel-body">
					<table id="a_group_tb" class="table table-striped table-bordered table-td-valign-middle">
						<thead>
							<tr>
								<th width="1%"></th>
								<th width="1%" data-orderable="false">avatar</th>
								<th class="text-nowrap">ID</th>
								<th class="text-nowrap">Name</th>
								<th class="text-nowrap">Local</th>
								<th class="text-nowrap">Member</th>
								<th class="text-nowrap">Leader ID</th>
							</tr>
						</thead>
						<tbody>
                            <%(allGroup != undefined) ? allGroup.forEach(function(item){ %>
							<tr class="odd gradeX">
								<td width="1%" class="f-s-600 text-inverse">1</td>
								<td width="1%" class="with-img"><img src="../../assets/avatars/<%= item.groups.avt %>" class="img-rounded height-30 width-30" /></td>
								<td><%= item.groups.code %></td>
								<td><%= item.groups.name %></td>
								<td><%= item.groups.local %></td>
								<td><%= item.groups.members.length %></td>
								<td><%= item.groups.members[0] %></td>
                            </tr>
                            <% }): "" %>
						</tbody>
					</table>
				</div>
			</div>
			<!-- end panel -->
            <!-- begin panel -->
			<div class="panel panel-inverse">
				<h3 class="p-10">Leader Information</h3>
				<!-- begin panel-body -->
				<div class="panel-body">
					<table id="a_leader_tb" class="table table-striped table-bordered table-td-valign-middle">
						<thead>
							<tr>
								<th width="1%"></th>
								<th width="1%" data-orderable="false">avatar</th>
								<th class="text-nowrap">Name</th>
								<th class="text-nowrap">Email</th>
								<th class="text-nowrap">Mobile</th>
								<th class="text-nowrap">job</th>
								<th class="text-nowrap">group ID</th>
								<th class="text-nowrap">group Name</th>
							</tr>
						</thead>
						<tbody>
                            <%(allLeader != undefined) ? allLeader.forEach(function(item){ %>
							<tr class="odd gradeX">
								<td width="1%" class="f-s-600 text-inverse">1</td>
								<td width="1%" class="with-img"><img src="../../assets/avatars/<%= item.leader.avatar %>" class="img-rounded height-30 width-30" /></td>
								<td><%= item.leader.first_name %></td> <%= item.leader.last_name %></td>
								<td><%= item.leader.email %></td>
								<td><%= item.leader.mobile %></td>
								<td><%= item.leader.job %></td>
								<td><%= item.groupId %></td>
								<td><%= item.groupName %></td>
                            </tr>
                            <% }): "" %>
						</tbody>
					</table>
				</div>
			</div>
			<!-- end panel -->
		</div>
		<!-- end #content -->
		
		<!-- begin scroll to top btn -->
		<a href="javascript:;" class="btn btn-icon btn-circle btn-success btn-scroll-to-top fade" data-click="scroll-top"><i class="fa fa-angle-up"></i></a>
		<!-- end scroll to top btn -->
	</div>
	
	<!-- ================== BEGIN BASE JS ================== -->
	<script src="../../assets/js/app.min.js"></script>
	<script src="../../assets/js/theme/transparent.min.js"></script>
	<!-- ================== END BASE JS ================== -->
	
	<!-- ================== BEGIN PAGE LEVEL JS ================== -->
	<script src="../../assets/plugins/datatables.net/js/jquery.dataTables.min.js"></script>
	<script src="../../assets/plugins/datatables.net-bs4/js/dataTables.bootstrap4.min.js"></script>
	<script src="../../assets/plugins/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
	<script src="../../assets/plugins/datatables.net-responsive-bs4/js/responsive.bootstrap4.min.js"></script>
	<script src="../../custom/sidebar.js"></script>
	<<script src="/socket.io/socket.io.js"></script>
	<script src="../../custom/a-group.js"></script>
	<!-- ================== END PAGE LEVEL JS ================== -->
</body>
</html>